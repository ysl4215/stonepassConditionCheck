import org.springframework.stereotype.Service;

@Service
public class ConditionServiceImpl {
	
	
	@Inject

	private BoardDAO boardDAO;

	

	public List<BoardVO> getBoardList() throws Exception {

		return boardDAO.getBoardList();

	}



