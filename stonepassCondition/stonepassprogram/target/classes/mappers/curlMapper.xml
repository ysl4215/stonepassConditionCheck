    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.swempire.web.condition.curlMapper">
	<update id="serverCurlConnection" parameterType="com.swempire.web.condition.VO.ServerCurlVO">
		update servercurl set connection = #{connection}
