$(document).on('click', '#cencle', function(e) {
		e.preventDefault();
		window.open("about:blank", "_self").close();
	});

$(document).on('click', '#orga_modify', function(e) {
		$("#form_orga_modify").submit();
		window.open("about:blank", "_self").close();
	});

	
	
