package com.swempire.web.comm.util;

import org.springframework.scheduling.annotation.Scheduled;
import org.springframework.stereotype.Component;
import org.springframework.ui.Model;

import com.swempire.web.condition.VO.ConditionVO;
import com.swempire.web.condition.VO.EmailVO;
import com.swempire.web.condition.controller.RestConditionController;

@Component
public class Scheduler extends RestConditionController{

	
	@Scheduled(fixedDelay = 15000)
	public void autoUpdate() throws Exception {
		
		curlArray(null, null, null, null, null);
	}

	@Override
	public String[] curlArray(int[] bidArray, String values, ConditionVO conditionvo, Model model, EmailVO emailvo)
			throws Exception {
		
		return super.curlArray(bidArray, values, conditionvo, model, emailvo);
	}
	
	

	
}
