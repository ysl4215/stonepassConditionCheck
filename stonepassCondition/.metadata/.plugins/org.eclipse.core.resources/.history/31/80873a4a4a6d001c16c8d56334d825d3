$(document).on('click', '#cencle', function(e) {
	e.preventDefault();
	window.open("about:blank", "_self").close();
});

$(document).on('click', '#orga_modify', function(e) {
	alert(1111);
	var modify = document.querySelector('#orga_modify');
	var bid = modify.getAttribute('value');
	
	alert(modify);
	alert(bid);
	
	
	/*$("#form_orga_modify").submit();*/
	/*window.open("about:blank", "_self").close();*/
});


$(document).on('click', '#orga_delete', function(e) {

	var bid = document.getElementById('bid').value;

	$.ajax({
		url: 'conditionDelete',
		type: 'post',
		data: {
			bid: bid
		},
		success: function(result) {

			opener.parent.location.reload();
			window.close();
		},
		error: function() {
			consol.log("error");
		}

	});


});